    "public": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/UniqueSoftEurope/AzurePipelineExtension"
    },
    "categories": ["Azure Pipelines"],
    "icons": {
        "default": "Images\\backup_icon.png"
    },
