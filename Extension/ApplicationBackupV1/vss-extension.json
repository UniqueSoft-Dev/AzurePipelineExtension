{
    "manifestVersion": 1,
    "id": "ApplicationBackup",
    "publisher": "NeedCode",
    "version": "1.6.0",
    "name": "Application backup",
    "description": "Backup files, folder before update.",
    "public": true,
    "galleryFlags": [
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Backup",
        "Archive",
        "zip",
        "Application Management"
    ],
    "links": {
        "home": {
            "uri": "https://github.com/UniqueSoftEurope"
        },
        "getstarted": {
            "uri": "https://github.com/UniqueSoftEurope/AzurePipelineExtension/wiki/GetStarted"
        },
        "repository": {
            "url": "https://github.com/UniqueSoftEurope/AzurePipelineExtension"
        },
        "issues": {
            "uri": "https://github.com/UniqueSoftEurope/AzurePipelineExtension/issues"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/UniqueSoftEurope/AzurePipelineExtension"
    },
    "icons": {
        "default": "Images/backup_icon.png"
    },
    "screenshots": [
        {
            "path": "Images\\screenshot_settings.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "contributions": [
        {
            "id": "NeedCode-ApplicationBackup",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "BuildTasks"
            }
        }
    ],
    "files": [
        {
            "path": "BuildTasks"
        }
    ]
}